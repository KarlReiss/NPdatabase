            <tr><td align="left">chinese_name</td><td align="left">varchar(500)</td><td align="left">处方中文名</td></tr>
            <tr><td align="left">pinyin_name</td><td align="left">varchar(200)</td><td align="left">拼音名</td></tr>
            <tr><td align="left">english_name</td><td align="left">varchar(500)</td><td align="left">英文名</td></tr>
