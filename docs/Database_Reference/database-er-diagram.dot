    diseases [label=<
        <table border="1" cellborder="0" cellpadding="3" cellspacing="0" bgcolor="#FCE4EC">
            <tr><td colspan="3" bgcolor="#AD1457"><font color="white"><b>diseases</b> (疾病表)</font></td></tr>
            <tr><td colspan="3" bgcolor="#F8BBD0"><font color="#333"><i>~1,500 条记录 | 8 字段</i></font></td></tr>
            <tr><td align="left"><b>字段名</b></td><td align="left"><b>类型</b></td><td align="left"><b>说明</b></td></tr>
            <tr><td align="left"><u>id</u></td><td align="left">bigint</td><td align="left">主键ID</td></tr>
            <tr><td align="left">icd11_code</td><td align="left">varchar(50)</td><td align="left">ICD-11编码</td></tr>
            <tr><td align="left">disease_name</td><td align="left">varchar(500)</td><td align="left">疾病英文名</td></tr>
            <tr><td align="left">disease_name_zh</td><td align="left">varchar(500)</td><td align="left">疾病中文名</td></tr>
            <tr><td align="left">disease_category</td><td align="left">varchar(200)</td><td align="left">疾病分类</td></tr>
