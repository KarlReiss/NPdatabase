            <tr><td align="left">indications</td><td align="left">text</td><td align="left">主治</td></tr>
            <tr><td align="left">tcmid_id</td><td align="left">varchar(50)</td><td align="left">TCMID编号</td></tr>
            <tr><td align="left">created_at</td><td align="left">timestamp</td><td align="left">创建时间</td></tr>
            <tr><td align="left">updated_at</td><td align="left">timestamp</td><td align="left">更新时间</td></tr>
            <tr><td align="left">disease_icd11_category</td><td align="left">text</td><td align="left">ICD-11分类</td></tr>
            <tr><td align="left">reference</td><td align="left">text</td><td align="left">参考文献</td></tr>
        </table>
    >];

    // ============================================
